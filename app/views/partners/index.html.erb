<div class="page-header">
  <h1>Listar Parceiros</h1>
</div>

<div class="panel panel-default">
  <div class="panel-heading">
    <h3 class="panel-title">Parceiros</h3>
  </div>
  <div class="panel-body">
    <table class="table table-hover">
      <thead>
        <tr>
          <th>Nome</th>
          <th>url</th>
          <th></th>
          <th></th>
          <th></th>
        </tr>
      </thead>

      <tbody>
        <% @partners.each do |partner| %>
          <tr>
            <td><%= partner.name %></td>
            <td><%= partner.url %></td>
            <td><%= link_to 'Detalhes', partner %></td>
            <td><%= link_to 'Editar', edit_partner_path(partner) %></td>
            <td><%= link_to 'Deletar', partner, method: :delete, data: { confirm: 'Are you sure?' } %></td>
          </tr>
        <% end %>
      </tbody>
    </table>
  </div> <!-- End Panel -->
</div>

<br>

<%= link_to 'Incluir Parceiro', new_partner_path %>
